{
  "name": "pushing-hours-restriction",
  "version": "0.6.0",
  "private": true,
  "description": "Add pushing hours restriction to the Branch protection rule of the default branch",
  "main": "lib/index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "run-p lint:*",
    "lint:format": "prettier --check .",
    "lint:quality-check": "eslint src/**/*.ts __tests__/**/*.test.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamuyuuki/pushing-hours-restriction.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "hamuyuuki",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.3.0",
    "@actions/github": "5.0.0",
    "@octokit/auth-app": "3.1.0",
    "@octokit/rest": "18.5.3",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-http": "1.5.17",
    "cross-fetch": "3.1.4",
    "date-fns": "2.21.3",
    "date-fns-tz": "1.1.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-resolvers": "1.19.2",
    "@octokit/graphql-schema": "10.41.0",
    "@types/jest": "26.0.23",
    "@types/node": "12.20.13",
    "@typescript-eslint/parser": "4.25.0",
    "@vercel/ncc": "0.28.6",
    "eslint": "7.27.0",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-jest": "24.3.6",
    "jest": "26.6.3",
    "jest-circus": "27.0.1",
    "jest-date-mock": "1.0.8",
    "js-yaml": "4.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "ts-jest": "26.5.6",
    "ts-node": "10.0.0",
    "typescript": "4.2.4"
  }
}
