{
  "name": "pushing-hours-restriction",
  "version": "0.8.1",
  "private": true,
  "description": "Add pushing hours restriction to the Branch protection rule of the default branch",
  "main": "lib/index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "run-p lint:*",
    "lint:format": "prettier --check .",
    "lint:quality-check": "eslint src/**/*.ts __tests__/**/*.test.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamuyuuki/pushing-hours-restriction.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "hamuyuuki",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0",
    "@octokit/auth-app": "6.0.4",
    "@octokit/rest": "20.0.2",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-http": "1.5.17",
    "cross-fetch": "4.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-document-nodes": "4.0.6",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@octokit/graphql-schema": "15.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.25",
    "@typescript-eslint/parser": "7.1.1",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.57.0",
    "eslint-plugin-github": "4.10.2",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "jest-date-mock": "1.0.8",
    "js-yaml": "4.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.2"
  }
}
