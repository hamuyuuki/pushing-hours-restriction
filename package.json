    "@octokit/graphql": "^4.5.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.13.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.3.0",
    "jest-circus": "^26.4.2",
    "jest-date-mock": "^1.0.8",
