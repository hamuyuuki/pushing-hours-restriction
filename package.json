{
  "name": "pushing-hours-restriction",
  "version": "0.5.2",
  "private": true,
  "description": "Add pushing hours restriction to the Branch protection rule of the default branch",
  "main": "lib/index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "run-p lint:*",
    "lint:format": "prettier --check .",
    "lint:quality-check": "eslint src/**/*.ts __tests__/**/*.test.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamuyuuki/pushing-hours-restriction.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "hamuyuuki",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@octokit/auth-app": "^2.10.3",
    "@octokit/rest": "^18.5.3",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.21.1",
    "date-fns-tz": "^1.1.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-resolvers": "1.19.1",
    "@octokit/graphql-schema": "^10.37.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^15.6.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.13.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.3.0",
    "jest-circus": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "js-yaml": "^3.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.1",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
